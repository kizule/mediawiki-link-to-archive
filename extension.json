{
	"name": "Link to Archive",
	"author": "Pierre Rudloff",
	"license-name": "GPL-3.0",
	"version": "1.1.0",
	"descriptionmsg": "linktoarchive-desc",
	"url": "https://github.com/Archi-Strasbourg/mediawiki-link-to-archive",
	"Hooks": {
		"LinkerMakeExternalLink": [
			"LinkToArchive\\LinkToArchive::onLinkerMakeExternalLink"
		],
		"BeforePageDisplay": [
			"LinkToArchive\\LinkToArchive::onBeforePageDisplay"
		]
	},
	"AutoloadClasses": {
		"LinkToArchive\\LinkToArchive": "LinkToArchive.php"
	},
	"MessagesDirs": {
		"LinkToArchive": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.linkToArchive.styles": {
			"styles": [
				"css/linkToArchive.css"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "LinkToArchive/resources"
	},
	"manifest_version": 2
}
